{
    "name": "mylists",
    "private": true,
    "type": "module",
    "browserslist": [
        ">0.3%, defaults, supports es6-module"
    ],
    "scripts": {
        "test": "vitest",
        "dk": "drizzle-kit",
        "ui": "bunx shadcn@latest",
        "build:server": "vite build",
        "dev": "vite dev --port 3000",
        "cli": "bun --env-file=.env src/cli/index.ts",
        "build": "bun run build:server && bun run build:cli",
        "build:cli": "vite build --config vite.cli.config.ts",
        "email": "email dev -d src/lib/client/components/emails",
        "seed:achievements": "bun run cli -- seedAchievements -d",
        "migrate:db": "bun run --env-file=.env scripts/db-migrate-script.ts",
        "new:db": "bun run dk push --force && bun run seed:achievements && bun run cli -- calculateAchievements",
        "auth:generate": "bunx @better-auth/cli@latest generate --y --output src/lib/server/database/schema/auth.schema.ts"
    },
    "dependencies": {
